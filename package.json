{
  "name": "decode-city",
  "version": "1.0.0",
  "description": "",
  "main": "opus.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test": "npm run typecheck && npm run check-format && npm run build",
    "format": "prettier --write src/**/*.ts src/**/*.tsx",
    "check-format": "prettier-check src/**/*.ts src/**/*.tsx",
    "build": "rm -rf dist && npx parcel build  --global main --no-source-maps --no-content-hash --public-url https://rameshvarun.github.io/binary-inspector/ src/*.html",
    "start": "parcel --global main src/*.html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "utils": "^0.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "prettier-check": "^2.0.0",
    "typescript": "^3.5.3"
  }
}
