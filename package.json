{
  "name": "binary-inspectors",
  "version": "0.0.0",
  "description": "Web-based inspectors for binary formats.",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch --no-watchman",
    "format": "prettier --write src/**/*.ts src/**/*.tsx *.js",
    "check-format": "prettier-check src/**/*.ts src/**/*.tsx",
    "build": "rm -rf dist/ && webpack --env production",
    "start": "webpack-dev-server --open --env development"
  },
  "author": "Varun Ramesh",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^25.1.1",
    "chai": "^4.2.0",
    "query-string": "^6.10.1",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "jest": "^25.1.0",
    "prettier": "^1.18.2",
    "prettier-check": "^2.0.0",
    "ts-jest": "^25.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.5.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}
